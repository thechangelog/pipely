varnishtest "Test www redirects"

varnish v1 -vcl {
  vcl 4.1;

  include "disable-default-backend.vcl";
  include "www.vcl";
} -start

# test basic redirect returning different path and status code
client c1 {
  txreq -url "/" -hdr "Host: www.pipely.local"
  rxresp
  expect resp.status == 301
  expect resp.http.location == "https://pipely.local/"
} -run
